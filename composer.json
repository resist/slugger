{
    "name": "resist/slugger",
    "description": "Custom slugger and Symfony slugger proxy",
    "license": "GPL-3.0-only",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": ["src"]
    },
    "autoload-dev": {
        "psr-4": {
            "resist\\Slugger\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.4",
        "ext-mbstring": "*",
        "symfony/string": "^7.0|^8.0",
        "symfony/translation-contracts": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0"
    }
}
